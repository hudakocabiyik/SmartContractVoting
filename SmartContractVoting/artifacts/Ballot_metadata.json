		"devdoc": {
			"details": "Implements voting process along with vote delegation, proposal addition, vote retraction, and voting deadline.",
			"events": {
				"VotingResults(bytes32,uint256)": {
					"details": "Announce the winner of the voting process by emitting an event."
				}
			},
			"kind": "dev",
			"methods": {
				"addProposal(bytes32)": {
					"details": "Add a new proposal. Only the chairperson can call this.",
					"params": {
						"proposalName": "name of the new proposal"
					}
				},
				"constructor": {
					"details": "Create a new ballot to choose one of 'proposalNames' with a voting deadline.",
					"params": {
						"durationMinutes": "the time duration for voting in minutes",
						"proposalNames": "names of proposals"
					}
				},
				"delegate(address)": {
					"details": "Delegate your vote to the voter 'to'.",
					"params": {
						"to": "address to which vote is delegated"
					}
				},
				"giveRightToVote(address)": {
					"details": "Give 'voter' the right to vote on this ballot. May only be called by 'chairperson'.",
					"params": {
						"voter": "address of voter"
					}
				},
				"retractVote()": {
					"details": "Retract your vote, allowing you to vote again."
				},
				"vote(uint256)": {
					"details": "Give your vote to proposal 'proposals[proposal].name'.",
					"params": {
						"proposal": "index of proposal in the proposals array"
					}
				},
				"winnerName()": {
					"details": "Calls winningProposal() function to get the index of the winner and returns the name of the winner.",
					"returns": {
						"winnerName_": "the name of the winning proposal"
					}
				},
				"winningProposal()": {
					"details": "Computes the winning proposal taking all previous votes into account.",
					"returns": {
						"winningProposal_": "index of winning proposal in the proposals array"
					}
				}
